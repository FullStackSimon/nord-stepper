{
  "name": "@nordhealth/components",
  "version": "4.5.0",
  "description": "This package includes Nord Design System Web Components",
  "author": "Nordhealth <support@nordhealth.design>",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/src/index.d.ts",
  "type": "module",
  "customElements": "custom-elements.json",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "custom-elements.json"
  ],
  "scripts": {
    "start": "concurrently -k -n TSC,HTTP -c blue.bold,magenta.bold \"tsc -w --noEmit\" \"wds\"",
    "prebuild": "pnpm clean",
    "build": "rollup -c",
    "postbuild": "pnpm analyze && pnpm build:react-types && pnpm build:vue-types",
    "build:docker": "docker compose build test",
    "clean": "rm -rf lib/",
    "clean:snapshots": "rm -rf src/*/screenshots/*/failed/*.png",
    "pretest": "pnpm clean:snapshots",
    "test": "docker compose run --rm -e INCLUDE_VISUAL_REGRESSION test",
    "test:watch": "docker compose run --rm -e INCLUDE_VISUAL_REGRESSION test --watch",
    "test:capture": "pnpm test --update-visual-baseline",
    "test:local": "wtr --coverage --watch",
    "analyze": "custom-elements-manifest analyze --litelement",
    "scaffold": "node ./scripts/scaffold.mjs",
    "build:react-types": "node ./scripts/build-react-types.mjs",
    "build:vue-types": "node ./scripts/build-vue-types.mjs"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
    "@nordhealth/icons": "^3.11.0",
    "lit": "^3.3.0",
    "qr": "^0.4.2",
    "tinykeys": "^1.4.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.4",
    "@lit-labs/testing": "^0.2.7",
    "@nordhealth/css": "^4.1.0",
    "@nordhealth/fonts": "^3.0.3",
    "@nordhealth/themes": "^9.0.0",
    "@nordhealth/tokens": "^8.0.0",
    "@open-wc/eslint-config": "^12.0.3",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "~0.2.16",
    "@web/dev-server-rollup": "^0.6.4",
    "@web/test-runner": "~0.16.1",
    "@web/test-runner-commands": "~0.7.0",
    "@web/test-runner-visual-regression": "~0.8.3",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-lit": "^1.15.0",
    "eslint-plugin-lit-a11y": "^4.1.4",
    "eslint-plugin-wc": "^1.5.0",
    "glob": "^11.0.2",
    "prettier": "^3.3.3",
    "rollup": "~4.21.3",
    "rollup-plugin-lit-css": "^5.0.1",
    "rollup-plugin-minify-html-literals-v3": "^1.3.4",
    "rollup-plugin-summary": "^2.0.1",
    "tslib": "^2.7.0",
    "typescript": "~5.5.4"
  },
  "overrides": {
    "rollup-plugin-summary": {
      "rollup": "~4.21.3"
    }
  },
  "eslintConfig": {
    "ignorePatterns": [
      "coverage",
      "demo/index.html",
      "demo/**/*.js",
      "utils/**/*.js",
      "test-utils/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "project": [
        "packages/components/tsconfig.json"
      ]
    },
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ],
    "rules": {
      "no-shadow": "off",
      "lit/no-classfield-shadowing": "off",
      "lit-a11y/no-aria-slot": "off",
      "@typescript-eslint/no-shadow": 2,
      "lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ],
      "wc/guard-super-call": "off",
      "no-unused-vars": "off",
      "no-undef": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "no-redeclare": "off",
      "@typescript-eslint/no-redeclare": 2,
      "import/no-unresolved": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "ignorePackages": true
        }
      ],
      "no-plusplus": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/test-utils/**/*.{js,mjs,ts}",
            "**/src/**/*.test.{js,mjs,ts}",
            "**/demo/**/*.{html,js,mjs,ts}",
            "**/*.config.{html,js,mjs,ts}",
            "**/*.conf.{html,js,mjs,ts}",
            "**/utils/*.{js,mjs}"
          ]
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": 2,
      "default-case": "off",
      "@typescript-eslint/switch-exhaustiveness-check": 2,
      "no-useless-constructor": "off",
      "@typescript-eslint/no-useless-constructor": "error",
      "no-empty-function": "off",
      "@typescript-eslint/no-empty-function": 2
    },
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "rules": {
          "no-dupe-class-members": "off"
        }
      },
      {
        "files": [
          "*.test.ts"
        ],
        "rules": {
          "@typescript-eslint/no-non-null-assertion": "off"
        }
      }
    ]
  },
  "gitHead": "c63c3e35a87cbb0e1e7c33183fc9830b37422316"
}
